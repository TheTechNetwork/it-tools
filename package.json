  "scripts": {
    "dev": "env -u npm_config_verify_deps_before_run -u npm_config_npm_globalconfig -u npm_config__jsr_registry vite",
    "build": "vue-tsc --noEmit && env -u npm_config_verify_deps_before_run -u npm_config_npm_globalconfig -u npm_config__jsr_registry NODE_OPTIONS=--max_old_space_size=4096 vite build",
    "preview": "env -u npm_config_verify_deps_before_run -u npm_config_npm_globalconfig -u npm_config__jsr_registry vite preview --port 5050",
    "test": "npm run test:unit",
    "test:unit": "vitest --environment jsdom --run",
    "test:e2e": "playwright test",
    "test:e2e:dev": "BASE_URL=http://localhost:5173 NO_WEB_SERVER=true playwright test",
    "coverage": "vitest run --coverage",
    "typecheck": "vue-tsc --noEmit -p tsconfig.vitest.json --composite false",
    "lint": "eslint src",
    "script:create:tool": "node scripts/create-tool.mjs",
    "script:create:ui": "hygen generator ui-component",
    "release": "node ./scripts/release.mjs"
  },
